<UserControl
    x:Class="NapCatScript.Desktop.Views.LogView"
    xmlns:vm="clr-namespace:NapCatScript.Desktop.ViewModels"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="450"
    d:DesignWidth="800"
    x:DataType="vm:LogViewModel"
    mc:Ignorable="d"
    BorderBrush="AliceBlue"
    Margin="0"
    
    BorderThickness="0">
    <!--Opacity="50"-->
    <ScrollViewer>
        <!--<ListBox ItemsSource="{Binding Log}"></ListBox>-->
        <TextBox 
            BorderThickness="0" 
            IsReadOnly="True" 
            Text="{Binding LogString}">
        </TextBox>
    </ScrollViewer>
</UserControl>
