<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vm="clr-namespace:NapCatScript.Desktop.ViewModels.NetWorkModels"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:netWorkViews="clr-namespace:NapCatScript.Desktop.Views.NetWorkViews"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="NapCatScript.Desktop.Views.NetWorkViews.CreateViews.NetWorkCreateView"
             x:DataType="vm:NetWorkCreateViewModel">
    <UserControl.Resources>
        <netWorkViews:CreateNetWorkViewLocator x:Key="ViewLocator"></netWorkViews:CreateNetWorkViewLocator>
    </UserControl.Resources>
    <UserControl.Styles>
            <StyleInclude Source="../../Styles/NetWorkViewStyle.axaml"></StyleInclude>
            <Style Selector="ContentControl">
                <Setter Property="ContentTemplate" Value="{StaticResource ViewLocator}"/>
            </Style>
    </UserControl.Styles>
    <ScrollViewer>
        <StackPanel>
            <WrapPanel>
                <ListBox ItemsSource="{Binding CreateList}">
                    <ListBox.ItemsPanel>
                        <ItemsPanelTemplate>
                            <WrapPanel></WrapPanel>
                        </ItemsPanelTemplate>
                    </ListBox.ItemsPanel>
                </ListBox>
            </WrapPanel>
            <UserControl></UserControl>
        </StackPanel>
    </ScrollViewer>
</UserControl>
