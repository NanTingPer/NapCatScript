<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             xmlns:chatViews="clr-namespace:NapCatScript.Desktop.ChatViews"
             xmlns:chatMain="clr-namespace:NapCatScript.Desktop.ChatViews.ChatMain"
             x:DataType="chatMain:ChatMainViewModel"
             x:Class="NapCatScript.Desktop.ChatViews.ChatMainView">
    <Grid ColumnDefinitions="1.5*, 8.5*">
        <UserControl Grid.Column="0" Content="{Binding LeftViewModel}"/>
        <UserControl Grid.Column="1" Content="{Binding RightViewModel }"/>
    </Grid>
</UserControl>
